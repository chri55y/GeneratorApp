<p id="notice"><%= notice %></p>

<h1>Second Blogs</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @second_blogs.each do |second_blog| %>
      <tr>
        <td><%= second_blog.title %></td>
        <td><%= link_to 'Show', second_blog %></td>
        <td><%= link_to 'Edit', edit_second_blog_path(second_blog) %></td>
        <td><%= link_to 'Destroy', second_blog, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Second Blog', new_second_blog_path %>
